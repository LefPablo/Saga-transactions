<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                   https://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd">

    <changeSet id="ACCOUNTING-12.01.26" author="finskiypavel@gmail.com">
        <createTable tableName="accounting_request">
            <column name="id" type="BIGINT" generationType="ALWAYS" autoIncrement="true">
                <constraints nullable="false" primaryKey="true" primaryKeyName="pk_accounting_request"/>
            </column>
            <column name="request_id" type="BIGINT">
                <constraints nullable="false"/>
            </column>
            <column name="budget" type="DECIMAL(10, 2)">
                <constraints nullable="false"/>
            </column>
            <column name="status" type="VARCHAR(30)">
                <constraints nullable="false"/>
            </column>
        </createTable>

    </changeSet>
</databaseChangeLog>