<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                   https://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd">

    <changeSet id="PROFILER-12.01.26" author="finskiypavel@gmail.com">
        <createTable tableName="profiler_request">
            <column name="id" type="BIGINT" generationType="ALWAYS" autoIncrement="true">
                <constraints nullable="false" primaryKey="true" primaryKeyName="pk_profiler_request"/>
            </column>
            <column name="request_id" type="BIGINT">
                <constraints nullable="false"/>
            </column>
            <column name="cv_uuid" type="UUID">
                <constraints nullable="false"/>
            </column>
        </createTable>

    </changeSet>
</databaseChangeLog>